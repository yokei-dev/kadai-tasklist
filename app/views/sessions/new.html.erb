<h1>Log in</h1>


<%= form_with(url: login_path, scope: :session, local: true) do |f| %>
    
    <%= f.label :email, 'メール' %>
    <%= f.email_field :email %>
    
    <%= f.label :password, 'パスワード' %>
    <%= f.password_field :password %>
    
    <%= f.submit 'Log in' %>
    
<% end %>

<p>new user？<%= link_to 'Signup', signup_path %></p>